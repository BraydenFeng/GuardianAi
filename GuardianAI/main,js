const navbar = document.getElementById('navbar');

window.addEventListener('scroll', () => {
  if (window.scrollY > 50) {
    navbar.classList.add('py-2', 'shadow-md');
    navbar.classList.remove('py-6');
  } else {
    navbar.classList.add('py-6');
    navbar.classList.remove('py-2', 'shadow-md');
  }
});


const firstrandom = Math.floor(Math.random() *30) + 1;

const secondrandom = Math.floor(Math.random() *80) +1;

document.getElementById("1random_number").textContent = firstrandom;

document.getElementById("2random_number").textContent = secondrandom ;

